{
  "kind": "collectionType",
  "collectionName": "directory_categories",
  "info": {
    "singularName": "directory-category",
    "pluralName": "directory-categories",
    "displayName": "Directory Category",
    "description": "Organize directory entries into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "directories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::directory.directory",
      "mappedBy": "category"
    }
  }
}
